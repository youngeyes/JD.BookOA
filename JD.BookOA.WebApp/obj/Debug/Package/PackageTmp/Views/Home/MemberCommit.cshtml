        $(function () {
            var q1 = $("#q1").val();
            var q2 = $("#q2").val();
            var q3 = $("#q3").val();
            var q1id = "";
            var q2id = "";
            $.ajax({
                url: "/xml/Provinces.xml",
                dataType: 'xml',
                type: 'GET',
                timeout: 2000,
                success: function (xml) {
                    var val = "";
                    
                    val = '<option value="">请选择省</option>';
                    $(xml).find("Province").each(function (i) {
                        var pid = $(this).attr("ID");
                        var pname = $(this).attr("ProvinceName");
                        if (pname==q1) {
                            val += '<option selected="" value="' + pid + '">' + pname + '</option>';
                            q1id = pid;
                        } else {
                            val += '<option value="' + pid + '">' + pname + '</option>';
                        }
                    });
                    $("#quiz1").append(val);
                }
            });
            if (q2 != "") {
                $("#quiz2").html("");
                $.ajax({
                    url: "/xml/Cities.xml",
                    dataType: 'xml',
                    type: 'GET',
                    timeout: 2000,
                    success: function (xml) {
                        var val = "";
                        val = '<option value="">请选择市</option>';
                        $(xml).find("City").each(function (i) {
                            var pid = $(this).attr("PID");
                            if (pid == q1id) {
                                var cid = $(this).attr("ID");
                                var cname = $(this).attr("CityName");
                                if (cname == q2) {
                                    val += '<option selected="" value="' + cid + '">' + cname + '</option>';
                                    q2id = cid;
                                } else {
                                    val += '<option value="' + cid + '">' + cname + '</option>';
                                }
                            }
                        });
                        $("#quiz2").append(val);
                    }
                });
            }
            if (q3 != "") {
                $("#quiz3").html("");
                $.ajax({
                    url: "/xml/Districts.xml",
                    dataType: 'xml',
                    type: 'GET',
                    timeout: 2000,
                    success: function (xml) {
                        var val = "";
                        val = '<option value="">请选择县/区</option>';
                        $(xml).find("District").each(function (i) {
                            var cid = $(this).attr("CID");
                            if (cid == q2id) {
                                var did = $(this).attr("ID");
                                var dname = $(this).attr("DistrictName");
                                if (dname == q3) {
                                    val += '<option selected="" value="' + did + '">' + dname + '</option>';
                                } else {
                                    val += '<option value="' + did + '">' + dname + '</option>';
                                }
                            }
                        });
                        $("#quiz3").append(val);
                    }
                });
            }
            if (q3=="") {
                $("#quiz3").hide();
            }
        })
        $(function () {
            KindEditor.ready(function (Kfc) {
                var editor = Kfc.editor({
                    allowFileManager: true,
                    cssPath: '/editor/plugins/code/prettify.css',
                    uploadJson: '/KindEditor/UploadImage',
                    fileManagerJson: '/KindEditor/ProcessRequest'
                });
                Kfc('#test').click(function () {

                            clickFn: function (url, title) {
                                Kfc('#LAY_demo_upload').attr("src",url);
                                //$("#LAY_demo_upload")
                                editor.hideDialog();
                            }
                        });
                    });
                });

            });
        })
       
        function func1() {
            var vs = $('#quiz1 option:selected').val();
            $("#quiz2").html("");
            $.ajax({
                url: "/xml/Cities.xml",
                dataType: 'xml',
                type: 'GET',
                timeout: 2000,
                success: function (xml) {
                    var val = "";
                    var p = $("#q2").val();
                    val = '<option value="">请选择市</option>';
                    $(xml).find("City").each(function (i) {
                        var pid = $(this).attr("PID");
                        if (pid == vs) {
                            var cid = $(this).attr("ID");
                            var cname = $(this).attr("CityName");
                            if (cname==p) {
                                val += '<option selected="" value="' + cid + '">' + cname + '</option>';
                            } else {
                                val += '<option value="' + cid + '">' + cname + '</option>';
                            }
                        }
                    });
                    $("#quiz2").append(val);
                }
            });
        }
        function func2() {
            var js = $('#quiz2 option:selected').val();
            $("#quiz3").html("");
            $.ajax({
                url: "/xml/Districts.xml",
                dataType: 'xml',
                type: 'GET',
                timeout: 2000,
                success: function (xml) {
                    var val = "";
                    var p = $("#q3").val();
                    val = '<option value="">请选择县/区</option>';
                    $(xml).find("District").each(function (i) {
                        var cid = $(this).attr("CID");
                        if (cid==js) {
                            var did = $(this).attr("ID");
                            var dname = $(this).attr("DistrictName");
                            if (dname==p) {
                                val += '<option selected="" value="' + did + '">' + dname + '</option>';
                            } else {
                                val += '<option value="' + did + '">' + dname + '</option>';
                            }
                        }
                    });
                    if (val == '<option value="">请选择县/区</option>') {
                        $("#quiz3").hide();
                    } else {
                        $("#quiz3").show();
                        $("#quiz3").append(val);
                    }
                   
                }
